#!finishing the backup tool for iOS 9.0.2

mkdir /var/mobile/Backup

mv "/var/mobile/'Package.deb'" "/var/mobile/Backup"

7z x 'Package.deb'

tar -zxvf data.tar.gz -C /var/mobile/Backup

rm -rf "/var/mobile/Backup/'Package.deb'" 

rm -rf "/var/mobile/Backup/'data.tar.gz'" 

rm -rf "/var/mobile/Backup/'control.tar.gz'" 

dpkg -i "/var/mobile/Backup/'*.deb'"

cp -a "/var/mobile/Backup/Preferences" "/var/mobile/Library/Preferences"

mkdir /var/mobile/Themes

mv "var/mobile/Backup/Themes" "/var/mobile/Themes"

rm -rf "/var/mobile/Backup"

killall -9 backboardd
