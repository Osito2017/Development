mkdir /var/mobile/F

mv "/var/mobile/'Target.deb'" "/var/mobile/F"

7zip x 'Target.deb'

tar -zxvf data.tar.gz -C /var/mobile/F

mv "/var/mobile/F/'Target.deb'" "/var/mobile"

mv "/var/mobile/F/'data.tar.gz'" "/var/mobile"

mv "/var/mobile/F/'control.tar.gz'" "/var/mobile"

dpkg -i "/var/mobile/F/'*.deb'"

killall -9 backboardd

